{
    "name": "tsitsulin/enum",
    "description": "PHP 8.1 compatible ENUM",
    "keywords": [
        "enum",
        "php 8.1",
        "polyfill"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Tsitsulin",
            "email": "tsitsulin@gmail.com",
            "homepage": "https://github.com/tsitsulin"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpstan/phpstan": "*",
        "codeception/codeception": "*",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Tsitsulin\\Enum\\": "src/"
        },
        "files": [
            "src/functions.php"
        ],
        "exclude-from-classmap": [
            "/tests/",
            "/tools/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tsitsulin\\Enum\\Examples\\": "examples/",
            "Tsitsulin\\Enum\\Tests\\": "tests/unit"
        }
    }
}